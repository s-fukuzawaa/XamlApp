<Page
    x:Class="Lab4.Food"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Lab4"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" >
            <Button Name="Button" FontFamily="Segoe MDL2 Assets" Content="&#xE700;" FontSize="36"  />
            <Button Name="Back" FontFamily="Segoe MDL2 Assets" Content="&#xE72B;" FontSize="36" Click="Back_Click" />

            <TextBlock Text=" Food " FontFamily="Arial" FontWeight="Bold" FontSize="28" VerticalAlignment="Center" />
        </StackPanel>
       
        <Frame Name="InnerFrame"/>
        <Image Source="Assets/Food.png" Grid.Row="1" HorizontalAlignment="Left" Stretch="Fill"></Image>

        
    </Grid>
</Page>
